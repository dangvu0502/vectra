{
  "name": "@querycub/server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "node --env-file=.env --import @swc-node/register/esm-register --watch ./src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "migrate:make": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:make",
    "migrate:latest": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:latest",
    "migrate:rollback": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:rollback"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.5",
    "@mastra/core": "^0.6.4",
    "@mastra/memory": "^0.2.5",
    "@mastra/pg": "^0.2.5",
    "@mastra/rag": "^0.1.13",
    "mastra": "^0.4.3",
    "ai": "^4.1.61",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.14.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.10",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.6"
  }
}
