{
  "name": "@vectra/server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "node --env-file=.env --import @swc-node/register/esm-register --watch ./src/main.ts",
    "prod": "node --import @swc-node/register/esm-register --watch ./src/main.ts",
    "build": "tsc --noEmitOnError false",
    "build:watch": "tsc --watch --noEmitOnError false",
    "start": "node --env-file=.env ./dist/main.js",
    "migrate:make": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:make",
    "migrate:latest": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:latest",
    "migrate:rollback": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:rollback",
    "migrate:down": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js migrate:down",
    "seed:make": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js seed:make",
    "seed:run": "node --env-file=.env --import @swc-node/register/esm-register ./node_modules/knex/bin/cli.js seed:run",
    "db:setup": "pnpm run db:orchestrate && pnpm run seed:run",
    "db:reset": "pnpm run db:delete:force && pnpm run db:orchestrate && pnpm run seed:run",
    "db:orchestrate": "node --env-file=.env --import @swc-node/register/esm-register ./src/database/scripts/orchestrate-db.ts",
    "db:delete:force": "node --env-file=.env --import @swc-node/register/esm-register ./src/database/scripts/force-delete-db.ts --force"
  },
  "dependencies": {
    "@llamaindex/ollama": "^0.1.2",
    "@mastra/core": "^0.6.4",
    "@mastra/memory": "^0.2.5",
    "@mastra/pg": "^0.2.5",
    "@mastra/rag": "^0.1.13",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "ai": "^4.1.61",
    "arangojs": "^10.1.1",
    "bullmq": "^5.10.3",
    "connect-redis": "^8.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "knex": "^3.1.0",
    "llamaindex": "^0.9.17",
    "mastra": "^0.4.3",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "ollama-ai-provider": "^1.2.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.14.1",
    "redis": "^4.6.15",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@types/arangodb": "^3.5.23",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.10",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.11.11",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.6"
  }
}
